{
  "ConnectionStrings": {
    "postgres": "Application Name=Elwark.People.Api.Background;User ID=master_account;Password=master_password;Host=localhost;Port=5432;Database=elwark.people.api;Pooling=false;"
  },
  "MessageQueue": {
    "Username": "rabbitmq",
    "Password": "rabbitmq",
    "VirtualHost": "elwark",
    "Port": 5672,
    "Hostnames": ["localhost"],
    "RequestTimeout": "00:10:00",
    "PublishConfirmTimeout": "00:10:00",
    "RecoveryInterval": "00:00:10",
    "PersistentDeliveryMode": true,
    "AutoCloseConnection": true,
    "AutomaticRecovery": true,
    "TopologyRecovery": true,
    "Exchange": {
      "Durable": true,
      "AutoDelete": false,
      "Type": "Direct"
    },
    "ExchangeName": "elwark_event_bus",
    "Queue": {
      "AutoDelete": false,
      "Durable": true,
      "Exclusive": false
    },
    "QueueName": "Elwark.People.Background",
    "ConnectionName": "Elwark.People.Background",
    "RetryCount": 10,
    "PrefetchCount": 30
  },
  "Urls": {
    "OAuthApi": "http://localhost:5001",
    "StorageApi": "http://localhost:3000",
    "IpInformationApi": "http://ip-api.com",
    "GravatarApi": "https://gravatar.com"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Error"
      }
    }
  }
}
